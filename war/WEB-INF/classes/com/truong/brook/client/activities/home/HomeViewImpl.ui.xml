<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:com.googlecode.mgwt.ui.client.widget.panel.scroll"
	xmlns:b="urn:import:com.googlecode.mgwt.ui.client.widget.button"
	xmlns:v="urn:import:com.truong.brook.client.view" xmlns:r="urn:import:com.googlecode.mgwt.ui.client.widget.input.radio">
	<ui:style>
	.homeView {
		position: relative;
	}
	
	.buttonView {
			border-radius: 20px !important; 
	}
		
	.container {
		background: transparent;
	}
	
	.content-primary {
		text-align: center !important;
	}
	</ui:style>
	<g:FlowPanel styleName="{style.homeView}" width="100%"
		ui:field="homeViewPanel">
		<g:VerticalPanel width="100%" ui:field="containerPanel"
			spacing="10" addStyleNames="{style.container}">
			<g:Button ui:field="btnTestConnect">Test connect server</g:Button>
		</g:VerticalPanel>
	</g:FlowPanel>
</ui:UiBinder> 